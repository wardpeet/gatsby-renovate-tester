      // update internal packages immediately after publish instead of waiting 3 days
      stabilityDays: 0,
    },
    {
      groupName: "starters and examples",
      paths: ["starters/**", "examples/**"],
      schedule: "before 3am on Monday",
    },
    {
      extends: ["monorepo:gatsby"],
